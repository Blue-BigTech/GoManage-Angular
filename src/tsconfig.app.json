{
  "compilerOptions": {
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "target": "es2020",
    "lib": [
      "es2020",
      "dom"
    ],
    "outDir": "../out-tsc/app",
    "module": "es2020",
    "baseUrl": "",
    "paths": {
      "@angular/*": ["./node_modules/@angular/*"]
    },
    "types": []
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
